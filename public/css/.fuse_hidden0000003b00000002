	width: 200px;
	height: 120px;
	top: 5%;
	left: 5%;
	position: absolute;
	-webkit-transform: rotate(-10deg);
	-ms-transform: rotate(-10deg);
	transform: rotate(-10deg);
	animation-name: triangle-box;
	animation-duration: 4s;
	border-color: #14acae;
	animation-iteration-count: infinite;
	animation-timing-function: ease-in-out;
	
}
polygon{
	stroke: #14acae;
	animation-name: triangle-svg;
	animation-duration: 14s;
	animation-iteration-count: infinite;
	z-index: -999;

}

@keyframes triangle-svg{
	0% {stroke: #2b2b2b; }
	25% {stroke: #83a6c9;}
	50% {stroke: #ae14ac; }
	75% {stroke: #97aa03; }
	100% {stroke: #8e1805;}
}

